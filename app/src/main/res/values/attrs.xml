<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FontTextView">
        <!-- This is mapped directly to the FontType.  If changes are made here, it should be made in FontType as well -->
        <attr name="fontName">
            <enum name="roboto_regular" value="0" />
            <enum name="roboto_light" value="1" />
            <enum name="roboto_light_italic" value="2" />
        </attr>
    </declare-styleable>
</resources>